{
	"name": "interface-conformance",
	"private": true,
	"displayName": "Interface Conformance for Go",
	"version": "0.0.5",
	"publisher": "CodeInbox",
	"icon": "docs/icon.png",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/magicbell-io/interface-conformance.git"
	},
	"engines": {
		"vscode": "^1.73.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:go"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run fetch-interfaces && npm run build",
		"build": "tsc -p ./src",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./src",
		"fetch-interfaces": "tsx ./scripts/fetch-interfaces.ts --outfile ./src/interfaces.json",
		"postinstall": "npm run fetch-interfaces",
		"package": "vsce package --no-dependencies",
		"publish": "vsce publish --no-dependencies"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"@types/tar": "^6.1.10",
		"@types/vscode": "^1.73.0",
		"@types/yargs": "^17.0.32",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/vsce": "^2.22.0",
		"eslint": "^8.26.0",
		"tar": "^6.2.0",
		"tsx": "^4.7.0",
		"typescript": "^5.3.2",
		"yargs": "^17.7.2"
	}
}
